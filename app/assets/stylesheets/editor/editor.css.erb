/* ====================================================================================================================================================
   Custom resets
   ==================================================================================================================================================== */

html, body {
  margin: 0;
  padding: 0;

  background-color: #000000;
  overflow: hidden;
}

menu, ol, ul, button, input, label {
  border: 0;
  margin: 0;
  padding: 0;
}

ol, ul, li {
  list-style: none;
}

h1, h2, h3, p {
  padding: 0;
  margin: 0;
}

button:focus {
  border: 0;
  outline: 0;
}

.btn {
    border-radius: 0;
}

a {
  color: #fff;
  text-decoration: none;
  font-family: 'Droid Sans', sans-serif;
}

a:last-child, button:last-child, img:last-child, p:last-child {
  margin-bottom: 0;
}

/* ====================================================================================================================================================
   Custom base layout, classes and conditions
   ==================================================================================================================================================== */

canvas {
  position: absolute;
  border-right: 2px solid;
  border-bottom: 2px solid;
  margin-left: 50%;
  left: -512px;
}

h1, h2, h3 {
  color: #FFE373;
  font-family: 'Droid Sans', sans-serif;
  font-weight: 400;
  background-image: url("<%= asset_path 'game/title_underline.png' %>");
  background-repeat: repeat-x;
  background-position: left bottom;
  margin-bottom: 15px;
}

h1 {
  font-size: 1.6em;
  padding-bottom: 1px;
  
}

h2 {
  font-size: 1.4em;
  padding-bottom: 2px;
}

h3 {
  font-size: 1.2em;
  padding-bottom: 3px;
}

h1 + p,
h2 + p,
h3 + p {
  margin-top: -4px;
}

p {
  padding: 0;
  margin-bottom: 20px;
  color: #F5F5F5;
  font-size: 12px;
  font-family: 'Droid Serif', sans-serif;
  line-height: 150%;
}

p + p {
  margin-top: -9px;
}

p + img {
  margin-top: -5px;
}

label {
  display: inline-block;
  color: #F5F5F5;
  font-size: 12px;
  font-family: 'Droid Sans', sans-serif;
  margin-bottom: 3px;
}

input {
  display: block;
  color: #1E1E1E;
  font-size: 12px;
  font-family: 'Droid Sans', sans-serif;
  border: 1px solid #A1A6AE;
  margin-left: 1px;
  height: 22px;
