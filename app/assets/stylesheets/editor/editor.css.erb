}

/* ==========================================================================
   Container, rows and columns
   ========================================================================== */

#ui-canvas {
  pointer-events: none;
}

#canvas-container {
  width: 1024px;
}

.container-fluid {
  padding: 0;
  margin-right: auto;
  margin-left: auto;
  overflow: hidden;
}

.row {
  clear: both;
}

.col {
  position: relative;
  background-color: #323232;
  min-height: 1px;
  height: 100%;
  float: left;
  overflow-x: hidden;
  overflow-y: auto;
}

/* ==========================================================================
   Panels
   ========================================================================== */

.panel {
  padding-left: 2px;
}

.panel-heading {
  background-color: #2574a9;
  margin: 0 0 2px 0;
  padding: 3px 0 0 7px;
  height: 27px;
}

