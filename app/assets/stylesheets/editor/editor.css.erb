  background-color: #323232;
}

.panel-title {
  color: #ffffff;
  font-size: 14px;
  font-family: 'Droid Sans', sans-serif;
  text-shadow: 1px 1px rgba(0, 0, 0, 0.2);
}

.panel-body {
  overflow-y: auto;
  margin: 0;
  padding: 18px 30px 30px 28px;
}

.panel-body h1:first-child {
  margin-top: -8px;
}

.panel-body h2:first-child {
  margin-top: -3px;
}

.panel-paragraph {
  padding-bottom: 10px;
  margin-bottom: 10px;
}

/* ==========================================================================
   Borders
   ========================================================================== */

.border-top {
  background-image: url("<%= asset_path 'game/border_top.png' %>");
  background-repeat: repeat-x;
  background-position: left top;
}

.border-bottom {
  background-image: url("<%= asset_path 'game/border_bottom.png' %>");
  background-repeat: repeat-x;
  background-position: left bottom;
}

.border-right {
  background-image: url("<%= asset_path 'game/border_right.png' %>");
  background-repeat: repeat-y;
  background-position: top right;
}

.border-left {
  background-image: url("<%= asset_path 'game/border_left.png' %>");
  background-repeat: repeat-y;
  background-position: top left;
}

/* ==========================================================================
   Toolbars
   ========================================================================== */

#toolbar-top {
  background-color: #323232;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  overflow: hidden;
  height: 86px;
}

#toolbar-bottom {
  position: absolute;
  top: 86px;
  right: 0;
  bottom: 0;
  left: 0;
}

#toolbar-bottom > .left {
  position: absolute;
  background-color: #323232;
  width: 50px;
  height: 100%;
}

#toolbar-bottom > .middle {
  position: absolute;
  top: 0;
  right: 330px;
  bottom: 0;
  left: 50px;
  background-color: #1B1B1B;
}

#toolbar-bottom > .right {
  position: absolute;
  background-color: #323232;
  width: 330px;
  height: 100%;
  right: 0;
  overflow: hidden;
}

#toolbar-scenes {
  background-color: #323232;
  height: 36px;
}

.scene-swap-highlight {
  border-right: 2px solid #FFFFFF;
}

/* ==========================================================================
   Dialogues
   ========================================================================== */

#dialogue-overlay {
  display: none;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.85);
  width: 100%;
  height: 100%;
  overflow: hidden;
  z-index: 1;

  -webkit-transition: all ease-out 0.2s;
  transition: all ease-out 0.2s;
}

.dialogue {
  background-color: #1E1E1E;
  position: absolute;
  overflow: hidden;
  z-index: 1;

  -webkit-transition: all ease-out 0.2s;
  transition: all ease-out 0.2s;
}

.dialogue > .panel {
  margin: 0;
  padding: 0;
}

#dialogue-window > .top {
  width: 100%;
  top: 0;
}

#dialogue-window > .right {
  height: 100%;
  right: 0;
}

#dialogue-window > .bottom {
  width: 100%;
  bottom: 0;
}

#dialogue-window > .left {
  height: 100%;
  left: 0;
}

#dialogue-window .btn {
  width: 100%;
  text-align: left;
  padding-left: 8px;
}

#dialogue-window .btn-normal {
  width: 120px;
  text-align: center;
  padding: 0;
}

#dialogue-window .nav-tabs > li {
